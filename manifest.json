{
  "name": "Commitment Alert!",
  "version": "1.89",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Commitment Alert!"
  },
  "content_scripts": [
    {
      "matches": [
        "https://raptor--icagentconsole.na137.visual.force.com/*",
        "https://*.my.salesforce.com/*/*"
      ],
      "js": [
        "libs/jquery/jquery-3.3.1.js",
        "DetectKeywordsEngine.js"
      ],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": [
      "libs/jquery/jquery-3.3.1.js",
      "KeywordSet.js"
    ]
  },
  "icons": {
    "16": "/icons/16x16.png",
    "48": "/icons/48x48.png",
    "128": "/icons/128x128.png"
  },
  "description": "Pop up sound alerts for commitments",
  "permissions": [
    "storage",
    "https://raptor--icagentconsole.na137.visual.force.com/*",
    "https://raptor.my.salesforce.com/*"
  ]
}